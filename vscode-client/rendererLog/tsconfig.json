{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2022",
    "moduleResolution": "NodeNext",
    "allowJs": false,
    // "composite": true,
    "outDir": "../../out/rendererLog",
    "rootDir": ".",
    "lib": [
      "ES7",
      "DOM",
      "ES2018",
      "ES2019",
      "ES2020",
      "ES2021",
      "ES2022",
    ],
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
  },
  "exclude": [
    ".vscode-test",
  ]
}